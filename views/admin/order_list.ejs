  <!-- END MENU SIDEBAR-->
    <div class="page-wrapper">
      <!-- HEADER MOBILE-->
      <header class="header-mobile d-block d-lg-none">
          <div class="header-mobile__bar">
              <div class="container-fluid">
                  <div class="header-mobile-inner">
                      <a class="logo" href="/admin/index">
                      </a>
                      <button class="hamburger hamburger--slider" type="button">
                          <span class="hamburger-box">
                              <span class="hamburger-inner"></span>
                          </span>
                      </button>
                  </div>
              </div>
          </div>
          <nav class="navbar-mobile">
              <div class="container-fluid">
                  <ul class="navbar-mobile__list list-unstyled">
                      <li>
                        <a href="/admin/index">
                            <i class="fas fa-desktop"></i>관리자 메인 화면</a>
                      </li>
                      <li>
                          <a href="/admin/shop">
                              <i class="fas fa-chart-bar"></i>상품 관리</a>
                      </li>
                      <li>
                          <a href="/admin/table">
                              <i class="fas fa-table"></i>공지 이벤트</a>
                      </li>
                      <li class="has-sub">
                          <a class="js-arrow" href="#">
                              <i class="fas fa-tachometer-alt"></i>상품 판매 정보 관리</a>
                          <ul class="navbar-mobile-sub__list list-unstyled js-sub-list">
                              <li>
                                  <a href="/admin/sales_statistics">주문 내역 확인</a>
                              </li>
                              <li>
                                  <a href="/admin/sales_statistics">판매 정보 통계</a>
                              </li>
                          </ul>
                      </li>
                      <li>
                          <a href="/users/logout">
                              <i class="zmdi zmdi-power"></i>로그아웃</a>
                      </li>
                  </ul>
              </div>
          </nav>
      </header>
      <!-- END HEADER MOBILE-->


        <!--PAGE CONTAINER-->
        <div class="page-container">
            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="row">

                        <div class="row">
                            <div class="col-lg-6">

                                <!-- END USER DATA-->
                            </div>
                            <div class="col-lg-6">

                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="col-xl-auto">
                                <!-- DATA TABLE -->
                                <h3 class="title-5 m-b-35">
                                  <!-- Start Wishlist  -->
                                  기간:
                                  <form method="post">
    	                               <input type="date">~<input type="date">&nbsp&nbsp<input type="submit" value="Submit"></form>

                                     <div class="wishlist-box-main">
                                      <div class="container">
                                          <div class="row">
                                              <div class="col-lg-12">
                                                  <div class="table-main table-responsive">
                                                      <table class="table" style="text-align: center;">
                                                          <thead>
                                                              <tr>
                                                                  <th>날짜</th>
                                                                  <th>상품명 </th>
                                                                  <th>가격 </th>
                                                                  <th>주문 상태</th>
                                                                  <th>비고</th>
                                                              </tr>
                                                          </thead>
                                                          <tbody>
                                                              <% order.forEach(function(key){ %> 
                                                                 <tr>
                                                                     <td class="thumbnail-img">
                                                                         <a href="#">
                                                                             <%= key.payDate %>
                                                                         </a>
                                                                     </td>
                                                                     <td class="name-pr">
                                                                             <a href="#" style="display:block;overflow: hidden; text-overflow: ellipsis;white-space: nowrap; width: 100px;">
                                                                                
                                                                                     <% var itemName = Object.values(key.cart.items) %>
                                                                                     <% itemName.forEach(function(rekey){ %> 
                                                                                         <%= rekey.item.name %>
                                                                                     <% }) %> 
                                                                            
                                                                             </a>
                                                                          외 &nbsp;
                                                                         <span style="text-align: right;">
                                                                             <%= (key.cart.totalQty) -1 %> 개
                                                                         </span>
                                                                     </td>
                                                                     <td class="price-pr">
                                                                         <p><%= key.cart.totalPrice %></p>
                                                                     </td>
                                                                     <td class="quantity-box"><%= key.flag %></td>
                                                                     <td class="add-pr">
                                                                         <% if(key.flag === "결제완료") { %>
                                                                            <form action="/admin/order_list/<%= key._id %>/complete" method="POST">
                                                                                <button type="submit" class="btn hvr-hover"  style="color: aliceblue;">배송하기</button>
                                                                            </form>
                                                                        <% } %>
                                                                        <% if(key.flag === "배송중") { %>
                                                                            <form action="/admin/order_list/<%= key._id %>/ing" method="POST">
                                                                                <button type="submit" class="btn hvr-hover" style="color: aliceblue;">배송완료</button>
                                                                            </form>
                                                                        <% } %>
                                                                        <% if(key.flag === "배송완료" || key.flag === "구매확정") { %>
                                                                            <form action="/admin/order_list/<%= key._id %>/end" method="POST">
                                                                                <button type="submit" class="btn hvr-hover" style="color: aliceblue;">상품평</button>
                                                                            </form>
                                                                        <% } %>
                                                                     </td>
                                                                 </tr>
                                                              <% }) %>
                                                          </tbody>
                                                      </table>
                                                  </div>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
    <!-- End Wishlist -->
                                </h3>
                                <div class="table-data__tool">
                                </div>
                                <div class="table-responsive table-responsive-data2">
                                </div>
                                <!-- END DATA TABLE -->
                            </div>
                        </div>
                        <div class="row m-t-30">
                            <div class="col-md-12">

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>








</body>
